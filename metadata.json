{
  "name": "puppet-winlogbeat",
  "version": "1.0.2-rc0",
  "author": "Vox Pupuli",
  "summary": "A module to install and manage the winlogbeat log shipper",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-winlogbeat",
  "project_page": "https://github.com/voxpupuli/puppet-winlogbeat",
  "issues_url": "https://github.com/voxpupuli/puppet-winlogbeat/issues",
  "tags": [
    "winlogbeat",
    "logstash",
    "elasticsearch",
    "elastic"
  ],
  "operatingsystem_support": [
    {
